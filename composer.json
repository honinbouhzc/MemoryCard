{
    "name": "scrmhub/activation-sdk",
    "description": "Activation SDK for SCRM Hub",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "silex/silex": "~2.0",
        "symfony/browser-kit": "~2.8|^3.0",
        "symfony/class-loader": "~2.8|^3.0",
        "symfony/config": "~2.8|^3.0",
        "symfony/console": "~2.8|^3.0",
        "symfony/css-selector": "~2.8|^3.0",
        "symfony/debug": "~2.8|^3.0",
        "symfony/finder": "~2.8|^3.0",
        "symfony/form": "~2.8|^3.0",
        "symfony/http-kernel": "~2.8|^3.0",
        "symfony/monolog-bridge": "~2.8|^3.0",
        "symfony/process": "~2.8|^3.0",
        "symfony/routing": "~2.8|^3.0",
        "symfony/security": "~2.8|^3.0",
        "symfony/translation": "~2.8|^3.0",
        "symfony/twig-bridge": "~2.8|^3.0",
        "symfony/validator": "~2.8|^3.0",
        "symfony/yaml": "~2.8|^3.0",
        "symfony/security-csrf": "~2.8|^3.0",

        "doctrine/inflector": "~1.0.1",
        "mheap/Silex-Assetic": "^2.0",
        "leafo/lessphp": "^0.5.0",
        "kriswallsmith/assetic": "^1.2",
        "knplabs/knp-components": "^1.3",
        "guzzlehttp/guzzle": "^6.0",
        "justinrainbow/json-schema": "~1.3",
        "scrmhub/api-php-sdk": "^1.1",
        "mobiledetect/mobiledetectlib": "^2.8",
        "tedivm/stash": "0.13.*",
        "xiaosier/libweibo": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "SCRMHub\\ActivationSDK\\": "framework"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://bitbucket.org/scrmhub/api-php-sdk.git"
        }
    ],
    "config": {
        "optimize-autoloader": false,
        "prepend-autoloader": true
    }
}
