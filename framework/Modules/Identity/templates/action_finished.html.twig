{% extends "layout.html.twig" %}

{% block content %} 
    <script>
        if (window.isWechat) {
            document.location = '{{ referrer }}';
        } else {
            window.opener.shareCompleted({
                'network': '{{ network }}',
                'module_id': '{{ module_id }}'
            });
            window.close();
        }
    </script>
{% endblock %}
